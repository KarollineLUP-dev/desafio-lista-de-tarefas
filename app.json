{
  "expo": {
    "name": "desafio-lista-de-tarefas",
    "slug": "desafio-lista-de-tarefas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-cejam.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-cejam.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.desafiolistadetarefas"

    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-cejam.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-cejam.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
